props ='initial_value'
var ='ini'
pipeline {
agent any
 stages {
       stage ('Testing Stage'){
     steps{
      script{
      def d = [test: 'Default', something: 'Default', other: 'Default']
      props = readProperties defaults: d, file: 'failedtestcases.properties', text: 'other=Override'
       }
       }
     }
    stage('two') {
      steps {
      script{
       var = props['failedtestcases'] 
       }
      }
    }
     stage ('Build') {
            steps {
                sh 'mvn clean install test -Dcucumber.options="--tags ${var}"' 
            }
      }
   }
 }
